3.33 565 admit 
3.66 420 notadmit
;
RUN;

*normalize here;

*fit the null model with no predictors;
PROC GENMOD DATA= one DESCENDING;
MODEL Status = /DIST= multinomial LINK= cumcll;
RUN;

*fit the full model with all predictors;
PROC GENMOD DATA= one DESCENDING;
MODEL Status = GPA GMAT /DIST= multinomial LINK= cumcll;
RUN;

DATA dev_one;
deviance=-2*((-43.0673)-(-26.3098));
pvalue=1-probchi(deviance,2);
RUN;

PROC PRINT DATA= dev_one;
RUN;

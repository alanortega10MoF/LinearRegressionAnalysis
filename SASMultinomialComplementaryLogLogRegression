PROC FORMAT;
VALUE Status$ sts_fmt 
'1notadmit'='notadmit'
'2border'='border'
'3admit' ='admit';
RUN;

DATA one;
LENGTH Status $9.0;
INPUT GPA GMAT Status$;
DATALINES;
2.96 596 admit 
2.8 444 border
3.14 473 admit 
3.13 416 border
